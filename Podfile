source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
inhibit_all_warnings!
use_frameworks!

def tapTalk_pods
    pod 'AFNetworking', '~> 3.1.0'
    pod 'SocketRocket'
    pod 'JSONModel', '~> 1.1'
    pod 'Realm'
    pod 'ZBarSDK', '~> 1.3'
    pod 'PodAsset'
    pod 'SDWebImage', '~> 4.0'
end

target "TapTalk" do
    tapTalk_pods
end
